{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "image-route": {
        "ClusterId": "fastapi-cluster",
        "Match": {
          "Path": "/image/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "fastapi-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:8000/"
          }
        }
      }
    }
  }

}
